<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
                           http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">

    <bean id="calculadora" class="br.edu.ifpi.capar.cdi.livro.negocio.impl.CalculadoraDeSalariosPlano2002">
        <property name="tabelaDeReferenciaSalarial">
            <ref bean="tabela"/>
        </property>
    </bean>
    
    <bean id="tabela" class="br.edu.ifpi.capar.cdi.livro.negocio.impl.TabelaDeReferenciaSalarialPadrao">
    </bean>
</beans>
